@article{sommer_new_1994,
	title = {A {New} {Way} to {Set} the {Energy} {Scale} in {Lattice} {Gauge} {Theories} and its {Application} to the {Static} {Force} and \${\textbackslash}textbackslashalpha\_s\$ in {SU}(2) {Yang}–{Mills} {Theory}},
