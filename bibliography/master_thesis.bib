
@article{shindler_nucleon_2015,
	title = {The nucleon electric dipole moment with the gradient flow: the \${\textbackslash}textbackslashtheta\$-term contribution},
	volume = {92},
	issn = {1550-7998, 1550-2368},
	shorttitle = {The nucleon electric dipole moment with the gradient flow},
	url = {http://arxiv.org/abs/1507.02343},
	doi = {10.1103/PhysRevD.92.094518},
	abstract = {We propose a new method to calculate electric dipole moments induced by the strong QCD \${\textbackslash}textbackslashtheta\$-term. The method is based on the gradient flow for gauge fields and is free from renormalization ambiguities. We test our method by computing the nucleon electric dipole moments in pure Yang-Mills theory at several lattice spacings, enabling a first-of-its-kind continuum extrapolation. The method is rather general and can be applied for any quantity computed in a \${\textbackslash}textbackslashtheta\$ vacuum. This first application of the gradient flow has been successful and demonstrates proof-of-principle, thereby providing a novel method to obtain precise results for nucleon and light nuclear electric dipole moments.},
	number = {9},
	urldate = {2018-04-25},
	journal = {Physical Review D},
	author = {Shindler, Andrea and Luu, Thomas and de Vries, Jordy},
	month = nov,
	year = {2015},
	note = {arXiv: 1507.02343},
	keywords = {High Energy Physics - Lattice}
}

